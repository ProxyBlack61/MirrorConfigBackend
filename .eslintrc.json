{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "semi": [2, "always"],
        "comma-dangle": [2, "always-multiline"]
    }
}
